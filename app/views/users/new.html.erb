<h1>Sign Up</h1>

<%= form_for :user, url: users_path,method: :post do |f| %>
  <%= f.label :nickname %><br>
  <%= f.text_field :nickname %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.submit %>
<%end %>